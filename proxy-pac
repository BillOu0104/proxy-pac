function FindProxyForURL(url, host) {
    // 直连本地和内网
    if (isPlainHostName(host) ||
        dnsDomainIs(host, ".local") ||
        isInNet(host, "10.0.0.0", "255.0.0.0") ||
        isInNet(host, "192.168.0.0", "255.255.0.0") ||
        isInNet(host, "172.16.0.0", "255.240.0.0")) {
        return "DIRECT";
    }

    // 需要代理的域名列表（支持子域名）
    var proxyList = [
        "doubao.com",
        "deepseek.com",
        "bilibili.tv"
    ];
    }

    return "DIRECT";
}
